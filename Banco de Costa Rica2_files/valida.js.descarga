function getTodaysDate(eField) {

var now = new Date();
var dia = now.getDate();
var mes = now.getMonth();
var ano = now.getYear();
var sano;
var sdia;
var smes;
var fecha;
var bNetscape;
var imes;
      
     sano = String(ano);
     sdia = String(dia);
     smes = String(mes);
     //0=Enero 1=Febrero ... 11=Noviembre
     imes = parseInt(smes);
     imes = imes + 1;
     smes = String(imes);
     fecha = String(fecha);
    
  
    if (eField == 'date03') 
      {        

	if (sdia.length == 1) 
            sdia = "0" + sdia;
        if (smes.length == 1)
            smes = "0" + smes;  

	if (navigator.appName == "Netscape") 
	  sano = sano.substr(1);
	else 
	  sano = sano.substr(2); 	    

        fecha = sdia +  smes + sano;
	eField = fecha;         
	return eField; 
        }  
}

