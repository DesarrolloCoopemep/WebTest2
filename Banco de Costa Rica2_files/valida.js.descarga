function isZero(Monto){
 var Limite;
 var ch;
 var i;

 Limite = Monto.length;
 for (i = 0;  i < Limite;  i++) {
    ch = Monto.charAt(i);
    if(ch!="0" && ch!="," && ch!="."){
	return false;
    }
 }  	

 return true;
}//isZero

