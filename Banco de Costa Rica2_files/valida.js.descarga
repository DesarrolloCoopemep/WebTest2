function isDigit(digito)
{
 var i;
 var valor;
 
 valor = true; 
 for (i = 0;  i < digito.length;  i++)
  {
    ch = digito.charAt(i);
    if (ch!="0" && ch!="1" && ch!="2" && ch!="3" && ch!="4" && ch!="5" && ch!="6" && ch!="7" && ch!="8" && ch!="9")
    {
       valor = false;
    }	
  }	
return valor;	
}//isDigit

